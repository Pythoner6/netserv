{
  "name": "netserv",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "import-k8s": "cdk8s import k8s@1.27.0",
    "synth-deps": "cdk8s synth --app 'npx ts-node deps.ts' --output dist-deps",
    "import": "cdk8s import",
    "synth": "cdk8s synth",
    "full-synth": "npm run import-k8s && npm run synth-deps && npm run import && npm run synth",
    "upgrade": "npm i cdk8s@latest cdk8s-cli@latest",
    "upgrade:next": "npm i cdk8s@next cdk8s-cli@next"
  },
  "dependencies": {
    "cdk8s": "^2.64.18",
    "cdk8s-plus-27": "^2.7.18",
    "constructs": "^10.2.70"
  },
  "devDependencies": {
    "cdk8s-cli": "^2.98.0",
    "@types/node": "^14",
    "@types/jest": "^26",
    "jest": "^26",
    "ts-jest": "^26",
    "typescript": "^4.9.5",
    "ts-node": "^10"
  }
}
